{
    "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "declaration": true,
    "removeComments": true,
    "noLib": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "rootDir": "src",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitUseStrict": false,
    "noFallthroughCasesInSwitch": true,
    "allowJs": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["es2015.collection","es2015.core","es5", "dom"]
    },
    "exclude": [
    "node_modules",
    "dist",
    "public",
    "coverage",
    "test-report"
    ],
    "compileOnSave": false
    }